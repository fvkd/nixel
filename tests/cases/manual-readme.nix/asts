---
LetIn {
 bindings: [
  Binding {
   from: AttributePath {
    parts: [
     Raw {
      content: "name",
      position: (3, 3),
     },
    ],
   },
   to: String {
    parts: [
     Raw {
      content: "John Doe",
      position: (3, 11),
     },
    ],
    position: (3, 10),
   },
  },
 ],
 target: String {
  parts: [
   Raw {
    content: "Hello, ",
    position: (4, 5),
   },
   Interpolation {
    expression: Variable {
     identifier: "name",
     position: (4, 14),
    },
   },
   Raw {
    content: "!",
    position: (4, 19),
   },
  ],
  position: (4, 4),
 },
 position: (1, 1),
}